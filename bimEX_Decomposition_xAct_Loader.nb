 "\t\t\t-- improved Echo"
}], "Subsubsection"],

Cell[BoxData[{
 RowBox[{"Clear", "[", "myEcho", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"myEcho", "[", "expr_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"l", ",", "ordinals", ",", "finalExpr", ",", "colour"}], "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"colour", "=", 
       RowBox[{"RGBColor", "[", 
        RowBox[{
         RowBox[{"255", "/", "255"}], ",", 
         RowBox[{"130", "/", "255"}], ",", 
         RowBox[{"70", "/", "255"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Head", "@", "expr"}], "===", "List"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"l", "=", 
          RowBox[{"Length", "@", "expr"}]}], ";"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"l", "=", "0"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"l", "\[Equal]", "0"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Echo", "@", "expr"}], ";"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"ordinals", "=", 
          RowBox[{"Array", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Row", "[", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"Style", "[", 
                 RowBox[{"\"\<(\>\"", ",", "colour"}], "]"}], ",", 
                RowBox[{"Style", "[", 
                 RowBox[{
                  RowBox[{"ToString", "[", "#", "]"}], ",", "colour"}], "]"}],
                 ",", 
                RowBox[{"Style", "[", 
                 RowBox[{"\"\<) \>\"", ",", "colour"}], "]"}]}], "}"}], "]"}],
              "&"}], ",", "l"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"finalExpr", "=", 
          RowBox[{"Partition", "[", 
           RowBox[{
            RowBox[{"Riffle", "[", 
             RowBox[{"ordinals", ",", "expr"}], "]"}], ",", "2"}], "]"}]}], 
         ";", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"finalExpr", "//", 
           RowBox[{
            RowBox[{"Row", "/@", "#"}], "&"}]}], "//", 
          RowBox[{
           RowBox[{"Echo", "/@", "#"}], "&"}]}], ";"}]}], 
       "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", "]"}]}], 
  ";"}]}], "Input",
