              RowBox[{"{", 
               RowBox[{
                RowBox[{"Style", "[", 
                 RowBox[{"\"\<(\>\"", ",", "colour"}], "]"}], ",", 
                RowBox[{"Style", "[", 
                 RowBox[{
                  RowBox[{"ToString", "[", "#", "]"}], ",", "colour"}], "]"}],
                 ",", 
                RowBox[{"Style", "[", 
                 RowBox[{"\"\<) \>\"", ",", "colour"}], "]"}]}], "}"}], "]"}],
              "&"}], ",", "l"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"finalExpr", "=", 
          RowBox[{"Partition", "[", 
           RowBox[{
            RowBox[{"Riffle", "[", 
             RowBox[{"ordinals", ",", "expr"}], "]"}], ",", "2"}], "]"}]}], 
         ";", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"finalExpr", "//", 
           RowBox[{
            RowBox[{"Row", "/@", "#"}], "&"}]}], "//", 
          RowBox[{
           RowBox[{"Echo", "/@", "#"}], "&"}]}], ";"}]}], 
       "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", "]"}]}], 
