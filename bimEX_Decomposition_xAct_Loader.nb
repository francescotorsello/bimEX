Cell[BoxData[
 RowBox[{
  RowBox[{"filterNonZeroNonEqual", "[", "exprList_", "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"es", "=", 
       RowBox[{"{", "0", "}"}]}], ",", 
      RowBox[{"e", "=", 
       RowBox[{"Flatten", "@", "exprList"}]}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"Not", "@", 
          RowBox[{"MemberQ", "[", 
           RowBox[{
            RowBox[{"Simplify", "[", 
             RowBox[{"Join", "[", 
              RowBox[{
               RowBox[{"Thread", "[", 
                RowBox[{"es", "-", 
                 SubscriptBox["e", 
                  RowBox[{
                  "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]]}], 
                "]"}], ",", 
               RowBox[{"Thread", "[", 
                RowBox[{"es", "+", 
                 SubscriptBox["e", 
                  RowBox[{
                  "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]]}], 
                "]"}]}], "]"}], "]"}], ",", "0"}], "]"}]}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"es", ",", 
           SubscriptBox["e", 
            RowBox[{"\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]]}],
           "]"}]}], "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"i", ",", 
         RowBox[{"Length", "@", "e"}]}], "}"}]}], "\[IndentingNewLine]", 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"es", "\[LeftDoubleBracket]", 
      RowBox[{"2", ";;"}], "\[RightDoubleBracket]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
